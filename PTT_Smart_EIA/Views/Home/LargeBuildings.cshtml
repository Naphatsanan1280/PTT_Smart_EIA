        }

        .page-header {
            font-size: 25px;
            width: 100%;
            height: 100px;
            background-color: rgba(112, 112, 112,1);
            line-height: 90px;
            text-align: center;
            position: fixed;
        }

        #homelabe {
            color: white;
        }

        .logo {
            padding: 5px;
            float: left;
            width: 50%;
            height: 100%;
            text-align: center;
