        #aa, #ab, #ac, #ad {
            border: solid 1px;
            padding: 5px;
        }

        #areatest, #costomtest, #kmztest {
            width: auto;
            background-color: azure;
        }
        .toppic {
            font-size: 40px;
            width: 100%;
            height: 100px;
            line-height: 90px;
            text-align: center;
            color: white;
            padding: 10px;
        }
        #contrian {
            width: 100%;
            height: 700px;
            padding: 10px;
        }

        select {
              display: block;
              margin: 0 auto;
             padding:5px;
