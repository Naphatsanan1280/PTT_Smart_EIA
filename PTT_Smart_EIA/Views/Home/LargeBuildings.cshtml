        }

        select {
            width: 100%;
        }

        #hrr {
            display: block;
            margin-top: 0.5px;
            margin-bottom: 0.5px;
            margin-left: auto;
            margin-right: auto;
            border-style: inset;
            border-width: 0.9px;
            background-color: black;
        }

    #resulte, #costomtest, #kmztest, #areatest ,#area ,#resulteNext,#b1,#b2,#b3{
            display: none;
        }

        #selectpanel {
            width: 100%;
            text-align: center;
            vertical-align: middle;
            display: flex;
            padding-left: 15px;
            padding-right: 15px;
        }

        #test {
            background-color: white;
            border: solid 1px;
            border-radius: 10px;
            width: 25%;
            height: 100%;
            float: left;
            padding: 10px;
