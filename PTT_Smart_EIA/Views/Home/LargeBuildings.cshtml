            <div class="user">
                <div id="aa">พิกัดที่ตั้ง </div>
                <div id="areatest">
                    <label>ระบุพิกัด UTM WGS 84</label>
                    <label>
                        Zone:
                        <select>
                            <option>47</option>
                            <option>48</option>
                        </select>
                    </label>
                    <hr />
                    <label>
                        East:
